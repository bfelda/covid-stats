{
	"name": "covid-data",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@react-google-maps/api": "^2.2.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-ga": "^3.3.0",
		"react-scripts": "4.0.3",
